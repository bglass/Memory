$('#jstree').jstree({ 
    core : {
        data : [
            { "id" : "1", "parent" : "#", "text" : "children" },
            { "id" : "2", "parent" : "1", "text" : "search" }
        ],
    },
    search : {
        search_callback : function (str, node) {
            if(node.text === str) { return true; }
        }
    },
    plugins : ['search']
}).on('ready.jstree', function(e, data) {
   data.instance.search("search");
});
